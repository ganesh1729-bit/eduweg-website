{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    },
    {
      "src": "static/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    { "src": "/submit-form", "dest": "/app.py" },
    { "src": "/(.*)", "dest": "/static/index.html" }
  ]
}